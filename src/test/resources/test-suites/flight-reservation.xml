<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.1.dtd" >
<suite name="Flight Reservation Suite" parallel="tests" thread-count="4">
    <test name="Passenger Count-1">
        <parameter name="testdatapath" value="FlightReservation/OnePassenger.json"></parameter>
        <classes>
            <class name="Tests.FlightReservation.FlightReservationTest"/>
        </classes>
    </test>
    <test name="Passenger Count-2">
        <parameter name="testdatapath" value="FlightReservation/TwoPassenger.json"></parameter>
        <classes>
            <class name="Tests.FlightReservation.FlightReservationTest"/>
        </classes>
    </test>
    <test name="Passenger Count-3">
        <parameter name="testdatapath" value="FlightReservation/ThirdPassenger.json"></parameter>
        <classes>
            <class name="Tests.FlightReservation.FlightReservationTest"/>
        </classes>
    </test>
    <test name="Passenger Count-4">
        <parameter name="testdatapath" value="FlightReservation/FourthPassenger.json"></parameter>
        <classes>
            <class name="Tests.FlightReservation.FlightReservationTest"/>
        </classes>
    </test>
</suite>